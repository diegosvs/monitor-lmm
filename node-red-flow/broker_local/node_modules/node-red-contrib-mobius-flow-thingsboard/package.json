{
  "name": "node-red-contrib-mobius-flow-thingsboard",
  "version": "1.0.8",
  "description": "Node-RED nodes to work with MOBiUSFlow and ThingsBoard.io",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && mkdir -p dist/nodes && cp src/nodes/*.html dist/nodes/ && cp -r src/nodes/icons dist/nodes/",
    "prepublishOnly": "tsc && mkdir -p dist/nodes && cp src/nodes/*.html dist/nodes/ && cp -r src/nodes/icons dist/nodes/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://chrismoorhouse@bitbucket.org/interiorautomation/node-red-contrib-mobius-flow-thingsboard.git"
  },
  "keywords": [
    "mobiusflow",
    "node-red",
    "thingsboard"
  ],
  "author": "Chris Moorhouse",
  "license": "BSD-3-Clause",
  "homepage": "https://bitbucket.org/interiorautomation/node-red-contrib-mobius-flow-thingsboard#readme",
  "node-red": {
    "nodes": {
      "mobius thingsboard connection": "dist/nodes/connection-manager.js",
      "mobius thingsboard manager": "dist/nodes/thingsboard-manager.js",
      "mobius thingsboard device publish": "dist/nodes/thingsboard-device-publish.js",
      "mobius thingsboard asset publish": "dist/nodes/thingsboard-asset-publish.js",
      "mobius thingsboard attribute subscribe": "dist/nodes/thingsboard-attribute-subscribe.js"
    }
  },
  "dependencies": {
    "mqtt": "^3.0.0",
    "node-rest-client": "^3.1.0"
  },
  "devDependencies": {
    "@ia/eslint-config-ia": "^1.0.2",
    "@types/node": "^13.9.1",
    "typescript": "^3.8.3"
  }
}
